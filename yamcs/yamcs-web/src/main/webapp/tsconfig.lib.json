{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/lib",
    "target": "es2015",
    "declaration": true,
    "inlineSources": true,
    "types": [],
    "lib": [
      "dom",
      "es2018"
    ]
  },
  "angularCompilerOptions": {
    "skipTemplateCodegen": false,
    "strictMetadataEmit": false,
    "fullTemplateTypeCheck": false,
    "enableResourceInlining": true,
    "enableIvy": false
  }
}
